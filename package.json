{
  "name": "webstore",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "nodemon src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "jasmine": "jasmine",
    "test": "npm run build && ENV=test npm run testdb",
    "testdb": "db-migrate --env test up && jasmine-ts --random=false && db-migrate --env test down",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess 'node ./dist/server.js'",
    "tsc": "tsc"
  },
