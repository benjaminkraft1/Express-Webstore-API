    "@types/morgan": "^1.9.3",
    "@types/node": "^16.9.5",
    "@types/pg": "^8.6.1",
    "@types/sharp": "^0.29.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
